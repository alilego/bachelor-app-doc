<% include ./partials/header %> 

<header id="myCarousel" class="carousel slide">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>

    <!-- Wrapper for Slides -->
    <div class="carousel-inner">
        <div class="item active" onclick="location.href='/map';">
            <!-- Set the third background image using inline CSS below. 1900x1080 resolution required -->
            <div class="fill" style="background-image:url('images/Poluare_crop.jpg');"></div>
            <!--div class="carousel-caption">
                <h2>Caption 1</h2>
            </div-->
        </div>
        <div class="item" onclick="location.href='/map';">
            <!-- Set the third background image using inline CSS below. 1900x1080 resolution required -->
            <div class="fill" style="background-image:url('images/poluare-masina.jpg');"></div>
        </div>
        <div class="item" onclick="location.href='/map';">
            <!-- Set the third background image using inline CSS below. 1900x1080 resolution required -->
            <div class="fill" style="background-image:url('images/masini-getty_crop.jpg');"></div>
        </div>
        <div class="item" onclick="location.href='/map';">
            <!-- Set the third background image using inline CSS below. 1900x1080 resolution required -->
            <div class="fill" style="background-image:url('images/human-lung.jpg');"></div>
        </div>
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="icon-prev"></span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="icon-next"></span>
    </a>

</header>

<script>
  
$(document).ready(function(){
   $("#footerNavbar").removeClass("navbar-fixed-bottom");
   $("#footerNavbar").addClass("navbar-bottom");
});
  
</script>

